#include "s21_tests.h"

#suite fmod
#test PI
    double a = 99999 * M_PI;
    double b = 2 * M_PI;
    
    double exp = fmod(a, b);
    double res = (double)s21_fmod(a, b);
    
    ck_assert_ldouble_eq_tol(exp, res, 10e-6);


#test arbitrary_pos_pos
    double a = 15.0/13.0;
    double b = 4.0/9.0;
    
    double exp = fmod(a, b);
    double res = (double)s21_fmod(a, b);
    
    ck_assert_ldouble_eq(exp, res);


#test arbitrary_pos_neg
    double a = 15.0/13.0;
    double b = -4.0/9.0;
    
    double exp = fmod(a, b);
    double res = (double)s21_fmod(a, b);
    
    ck_assert_ldouble_eq(exp, res);


#test arbitrary_neg_pos
