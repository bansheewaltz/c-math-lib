#include "s21_tests.h"
#include <stdio.h>
#include <math.h>

#suite exp
#test arb_pos
    double x = 15.0/13.0;
    
    double expected = exp(x);
    double res = s21_exp(x);
    
    ck_assert_double_eq_tol(expected, res, S21_TOLERANCE);
    

#test arb_neg
    double x = -15.0/13.0;
    
    double expected = exp(x);
    double res = s21_exp(x);
    
    ck_assert_double_eq_tol(expected, res, S21_TOLERANCE);


